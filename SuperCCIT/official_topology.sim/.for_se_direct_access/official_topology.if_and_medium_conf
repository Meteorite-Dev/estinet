			Set Node5_MAC8023_InterfaceID_1.NumCollision = off
			Set Node5_MAC8023_InterfaceID_1.NumUniInPkt = off
			Set Node5_MAC8023_InterfaceID_1.NumUniOutPkt = off
			Set Node5_MAC8023_InterfaceID_1.NumUniInOutPkt = off
			Set Node5_MAC8023_InterfaceID_1.NumBroInPkt = off
			Set Node5_MAC8023_InterfaceID_1.NumBroOutPkt = off
			Set Node5_MAC8023_InterfaceID_1.NumBroInOutPkt = off
