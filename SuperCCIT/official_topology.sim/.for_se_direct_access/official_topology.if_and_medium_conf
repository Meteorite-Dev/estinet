			Set Node5_MAC8023_InterfaceID_4.OutThrputLogFile = our_topology_test.mac8023_n5_i4_out_thrput.log
			Set Node5_MAC8023_InterfaceID_4.InOutThrputLogFile = our_topology_test.mac8023_n5_i4_inout_thrput.log
			Set Node5_MAC8023_InterfaceID_4.LinktoHub = 0

