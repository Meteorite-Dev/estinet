Create Node 6 as Open_vSwitch with name = Open_vSwitch6
	Define InterfaceID 1
		Module Interface : Node6_Interface_InterfaceID_1
			Set Node6_Interface_InterfaceID_1.tunnel_type = tap
			Set Node6_Interface_InterfaceID_1.nat_id = 0
			Set Node6_Interface_InterfaceID_1.if_type = 8023
			Set Node6_Interface_InterfaceID_1.if_name = eth0
			Set Node6_Interface_InterfaceID_1.max_qlen = 50
			Set Node6_Interface_InterfaceID_1.log_qlen = off
			Set Node6_Interface_InterfaceID_1.log_option = FullLog
			Set Node6_Interface_InterfaceID_1.samplerate = 1
			Set Node6_Interface_InterfaceID_1.logFileName = our_topology.interface_n6_i1_qlen.log
			Set Node6_Interface_InterfaceID_1.EnableClassify = on
			Set Node6_Interface_InterfaceID_1.ClassifyFileName = our_topology.n6_i1.flow_rule

		Module MAC8023 : Node6_MAC8023_InterfaceID_1
			Set Node6_MAC8023_InterfaceID_1.mac = 00:01:00:00:00:0d
			Set Node6_MAC8023_InterfaceID_1.lock_mac = off
			Set Node6_MAC8023_InterfaceID_1.PromisOpt = off
			Set Node6_MAC8023_InterfaceID_1.mode = full
			Set Node6_MAC8023_InterfaceID_1.log = off
			Set Node6_MAC8023_InterfaceID_1.logInterval = 1
			Set Node6_MAC8023_InterfaceID_1.NumCollision = off
			Set Node6_MAC8023_InterfaceID_1.NumUniInPkt = off
			Set Node6_MAC8023_InterfaceID_1.NumUniOutPkt = off
			Set Node6_MAC8023_InterfaceID_1.NumUniInOutPkt = off
			Set Node6_MAC8023_InterfaceID_1.NumBroInPkt = off
			Set Node6_MAC8023_InterfaceID_1.NumBroOutPkt = off
			Set Node6_MAC8023_InterfaceID_1.NumBroInOutPkt = off
			Set Node6_MAC8023_InterfaceID_1.NumDrop = off
			Set Node6_MAC8023_InterfaceID_1.InThrput = off
			Set Node6_MAC8023_InterfaceID_1.OutThrput = off
			Set Node6_MAC8023_InterfaceID_1.InOutThrput = off
			Set Node6_MAC8023_InterfaceID_1.UniInLogFile = our_topology.mac8023_n6_i1_uni_in.log
			Set Node6_MAC8023_InterfaceID_1.UniOutLogFile = our_topology.mac8023_n6_i1_uni_out.log
			Set Node6_MAC8023_InterfaceID_1.UniInOutLogFile = our_topology.mac8023_n6_i1_uni_inout.log
			Set Node6_MAC8023_InterfaceID_1.BroInLogFile = our_topology.mac8023_n6_i1_bro_in.log
			Set Node6_MAC8023_InterfaceID_1.BroOutLogFile = our_topology.mac8023_n6_i1_bro_out.log
			Set Node6_MAC8023_InterfaceID_1.BroInOutLogFile = our_topology.mac8023_n6_i1_bro_inout.log
			Set Node6_MAC8023_InterfaceID_1.CollLogFile = our_topology.mac8023_n6_i1_collision.log
			Set Node6_MAC8023_InterfaceID_1.DropLogFile = our_topology.mac8023_n6_i1_drop.log
