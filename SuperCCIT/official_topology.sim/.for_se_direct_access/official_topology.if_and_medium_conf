			Set Node6_MAC8023_InterfaceID_1.OutThrputLogFile = our_topology.mac8023_n6_i1_out_thrput.log
			Set Node6_MAC8023_InterfaceID_1.InOutThrputLogFile = our_topology.mac8023_n6_i1_inout_thrput.log
			Set Node6_MAC8023_InterfaceID_1.LinktoHub = 0

