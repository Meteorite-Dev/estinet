	Define InterfaceID 3
		Module Interface : Node4_Interface_InterfaceID_3
			Set Node4_Interface_InterfaceID_3.tunnel_type = tap
			Set Node4_Interface_InterfaceID_3.nat_id = 0
			Set Node4_Interface_InterfaceID_3.if_type = 8023
			Set Node4_Interface_InterfaceID_3.if_name = eth2
			Set Node4_Interface_InterfaceID_3.max_qlen = 50
			Set Node4_Interface_InterfaceID_3.log_qlen = off
			Set Node4_Interface_InterfaceID_3.log_option = FullLog
			Set Node4_Interface_InterfaceID_3.samplerate = 1
			Set Node4_Interface_InterfaceID_3.logFileName = our_topology.interface_n4_i3_qlen.log
			Set Node4_Interface_InterfaceID_3.EnableClassify = on
			Set Node4_Interface_InterfaceID_3.ClassifyFileName = our_topology.n4_i3.flow_rule

		Module MAC8023 : Node4_MAC8023_InterfaceID_3
			Set Node4_MAC8023_InterfaceID_3.mac = 00:01:00:00:00:09
			Set Node4_MAC8023_InterfaceID_3.lock_mac = off
			Set Node4_MAC8023_InterfaceID_3.PromisOpt = off
			Set Node4_MAC8023_InterfaceID_3.mode = full
			Set Node4_MAC8023_InterfaceID_3.log = off
			Set Node4_MAC8023_InterfaceID_3.logInterval = 1
			Set Node4_MAC8023_InterfaceID_3.NumCollision = off
			Set Node4_MAC8023_InterfaceID_3.NumUniInPkt = off
			Set Node4_MAC8023_InterfaceID_3.NumUniOutPkt = off
			Set Node4_MAC8023_InterfaceID_3.NumUniInOutPkt = off
			Set Node4_MAC8023_InterfaceID_3.NumBroInPkt = off
			Set Node4_MAC8023_InterfaceID_3.NumBroOutPkt = off
			Set Node4_MAC8023_InterfaceID_3.NumBroInOutPkt = off
			Set Node4_MAC8023_InterfaceID_3.NumDrop = off
			Set Node4_MAC8023_InterfaceID_3.InThrput = off
			Set Node4_MAC8023_InterfaceID_3.OutThrput = off
			Set Node4_MAC8023_InterfaceID_3.InOutThrput = off
			Set Node4_MAC8023_InterfaceID_3.UniInLogFile = our_topology.mac8023_n4_i3_uni_in.log
			Set Node4_MAC8023_InterfaceID_3.UniOutLogFile = our_topology.mac8023_n4_i3_uni_out.log
			Set Node4_MAC8023_InterfaceID_3.UniInOutLogFile = our_topology.mac8023_n4_i3_uni_inout.log
			Set Node4_MAC8023_InterfaceID_3.BroInLogFile = our_topology.mac8023_n4_i3_bro_in.log
			Set Node4_MAC8023_InterfaceID_3.BroOutLogFile = our_topology.mac8023_n4_i3_bro_out.log
			Set Node4_MAC8023_InterfaceID_3.BroInOutLogFile = our_topology.mac8023_n4_i3_bro_inout.log
			Set Node4_MAC8023_InterfaceID_3.CollLogFile = our_topology.mac8023_n4_i3_collision.log
			Set Node4_MAC8023_InterfaceID_3.DropLogFile = our_topology.mac8023_n4_i3_drop.log
			Set Node4_MAC8023_InterfaceID_3.InThrputLogFile = our_topology.mac8023_n4_i3_in_thrput.log
			Set Node4_MAC8023_InterfaceID_3.OutThrputLogFile = our_topology.mac8023_n4_i3_out_thrput.log
			Set Node4_MAC8023_InterfaceID_3.InOutThrputLogFile = our_topology.mac8023_n4_i3_inout_thrput.log
			Set Node4_MAC8023_InterfaceID_3.LinktoHub = 0

		Module PHY : Node4_PHY_InterfaceID_3
			Set Node4_PHY_InterfaceID_3.datarate = 10_Mbps
			Set Node4_PHY_InterfaceID_3.BER = 0.0
			Set Node4_PHY_InterfaceID_3.PropDelay = 1.0
			Set Node4_PHY_InterfaceID_3.linkfail = off
			Set Node4_PHY_InterfaceID_3.linkfailFileName = our_topology.phy8023_n4_i3.link_failure_duration

		Module Link : Node4_Link_InterfaceID_3
			Set Node4_Link_InterfaceID_3.guitag_remotenode = yes


		Bind Node4_Interface_InterfaceID_3 Node4_MAC8023_InterfaceID_3
		Bind Node4_MAC8023_InterfaceID_3 Node4_PHY_InterfaceID_3
		Bind Node4_PHY_InterfaceID_3 Node4_Link_InterfaceID_3
	EndDefine

	Define InterfaceID 4
		Module Interface : Node4_Interface_InterfaceID_4
			Set Node4_Interface_InterfaceID_4.tunnel_type = tap
			Set Node4_Interface_InterfaceID_4.nat_id = 0
			Set Node4_Interface_InterfaceID_4.if_type = 8023
			Set Node4_Interface_InterfaceID_4.if_name = eth3
			Set Node4_Interface_InterfaceID_4.max_qlen = 50
			Set Node4_Interface_InterfaceID_4.log_qlen = off
			Set Node4_Interface_InterfaceID_4.log_option = FullLog
			Set Node4_Interface_InterfaceID_4.samplerate = 1
			Set Node4_Interface_InterfaceID_4.logFileName = our_topology_test.interface_n4_i4_qlen.log
			Set Node4_Interface_InterfaceID_4.EnableClassify = on
			Set Node4_Interface_InterfaceID_4.ClassifyFileName = our_topology_test.n4_i4.flow_rule

		Module MAC8023 : Node4_MAC8023_InterfaceID_4
			Set Node4_MAC8023_InterfaceID_4.mac = 00:01:00:00:00:4f
			Set Node4_MAC8023_InterfaceID_4.lock_mac = off
			Set Node4_MAC8023_InterfaceID_4.PromisOpt = off
			Set Node4_MAC8023_InterfaceID_4.mode = full
			Set Node4_MAC8023_InterfaceID_4.log = off
			Set Node4_MAC8023_InterfaceID_4.logInterval = 1
			Set Node4_MAC8023_InterfaceID_4.NumCollision = off
			Set Node4_MAC8023_InterfaceID_4.NumUniInPkt = off
			Set Node4_MAC8023_InterfaceID_4.NumUniOutPkt = off
			Set Node4_MAC8023_InterfaceID_4.NumUniInOutPkt = off
			Set Node4_MAC8023_InterfaceID_4.NumBroInPkt = off
			Set Node4_MAC8023_InterfaceID_4.NumBroOutPkt = off
			Set Node4_MAC8023_InterfaceID_4.NumBroInOutPkt = off
			Set Node4_MAC8023_InterfaceID_4.NumDrop = off
			Set Node4_MAC8023_InterfaceID_4.InThrput = off
			Set Node4_MAC8023_InterfaceID_4.OutThrput = off
			Set Node4_MAC8023_InterfaceID_4.InOutThrput = off
			Set Node4_MAC8023_InterfaceID_4.UniInLogFile = our_topology_test.mac8023_n4_i4_uni_in.log
			Set Node4_MAC8023_InterfaceID_4.UniOutLogFile = our_topology_test.mac8023_n4_i4_uni_out.log
			Set Node4_MAC8023_InterfaceID_4.UniInOutLogFile = our_topology_test.mac8023_n4_i4_uni_inout.log
			Set Node4_MAC8023_InterfaceID_4.BroInLogFile = our_topology_test.mac8023_n4_i4_bro_in.log
			Set Node4_MAC8023_InterfaceID_4.BroOutLogFile = our_topology_test.mac8023_n4_i4_bro_out.log
			Set Node4_MAC8023_InterfaceID_4.BroInOutLogFile = our_topology_test.mac8023_n4_i4_bro_inout.log
			Set Node4_MAC8023_InterfaceID_4.CollLogFile = our_topology_test.mac8023_n4_i4_collision.log
			Set Node4_MAC8023_InterfaceID_4.DropLogFile = our_topology_test.mac8023_n4_i4_drop.log
			Set Node4_MAC8023_InterfaceID_4.InThrputLogFile = our_topology_test.mac8023_n4_i4_in_thrput.log
			Set Node4_MAC8023_InterfaceID_4.OutThrputLogFile = our_topology_test.mac8023_n4_i4_out_thrput.log
			Set Node4_MAC8023_InterfaceID_4.InOutThrputLogFile = our_topology_test.mac8023_n4_i4_inout_thrput.log
			Set Node4_MAC8023_InterfaceID_4.LinktoHub = 0

		Module PHY : Node4_PHY_InterfaceID_4
			Set Node4_PHY_InterfaceID_4.datarate = 10_Mbps
			Set Node4_PHY_InterfaceID_4.BER = 0.0
			Set Node4_PHY_InterfaceID_4.PropDelay = 1.0
			Set Node4_PHY_InterfaceID_4.linkfail = off
			Set Node4_PHY_InterfaceID_4.linkfailFileName = our_topology_test.phy8023_n4_i4.link_failure_duration

		Module Link : Node4_Link_InterfaceID_4
			Set Node4_Link_InterfaceID_4.guitag_remotenode = yes


		Bind Node4_Interface_InterfaceID_4 Node4_MAC8023_InterfaceID_4
		Bind Node4_MAC8023_InterfaceID_4 Node4_PHY_InterfaceID_4
		Bind Node4_PHY_InterfaceID_4 Node4_Link_InterfaceID_4
	EndDefine

EndCreate


Create Node 5 as Open_vSwitch with name = Open_vSwitch5
	Define InterfaceID 1
		Module Interface : Node5_Interface_InterfaceID_1
			Set Node5_Interface_InterfaceID_1.tunnel_type = tap
			Set Node5_Interface_InterfaceID_1.nat_id = 0
			Set Node5_Interface_InterfaceID_1.if_type = 8023
			Set Node5_Interface_InterfaceID_1.if_name = eth0
			Set Node5_Interface_InterfaceID_1.max_qlen = 50
			Set Node5_Interface_InterfaceID_1.log_qlen = off
			Set Node5_Interface_InterfaceID_1.log_option = FullLog
			Set Node5_Interface_InterfaceID_1.samplerate = 1
			Set Node5_Interface_InterfaceID_1.logFileName = our_topology.interface_n5_i1_qlen.log
