			Set Node8_MAC8023_InterfaceID_6.OutThrput = off
			Set Node8_MAC8023_InterfaceID_6.InOutThrput = off
			Set Node8_MAC8023_InterfaceID_6.UniInLogFile = our_topology_test.mac8023_n8_i6_uni_in.log
			Set Node8_MAC8023_InterfaceID_6.UniOutLogFile = our_topology_test.mac8023_n8_i6_uni_out.log
			Set Node8_MAC8023_InterfaceID_6.UniInOutLogFile = our_topology_test.mac8023_n8_i6_uni_inout.log
			Set Node8_MAC8023_InterfaceID_6.BroInLogFile = our_topology_test.mac8023_n8_i6_bro_in.log
			Set Node8_MAC8023_InterfaceID_6.BroOutLogFile = our_topology_test.mac8023_n8_i6_bro_out.log
			Set Node8_MAC8023_InterfaceID_6.BroInOutLogFile = our_topology_test.mac8023_n8_i6_bro_inout.log
			Set Node8_MAC8023_InterfaceID_6.CollLogFile = our_topology_test.mac8023_n8_i6_collision.log
			Set Node8_MAC8023_InterfaceID_6.DropLogFile = our_topology_test.mac8023_n8_i6_drop.log
