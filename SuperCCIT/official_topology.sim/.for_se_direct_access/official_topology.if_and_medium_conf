			Set Node5_MAC8023_InterfaceID_1.lock_mac = off
			Set Node5_MAC8023_InterfaceID_1.PromisOpt = off
			Set Node5_MAC8023_InterfaceID_1.mode = full
			Set Node5_MAC8023_InterfaceID_1.log = off
