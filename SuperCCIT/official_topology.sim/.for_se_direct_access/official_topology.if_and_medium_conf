Create Node 15 as Open_vSwitch with name = Open_vSwitch15
	Define InterfaceID 1
		Module Interface : Node15_Interface_InterfaceID_1
			Set Node15_Interface_InterfaceID_1.tunnel_type = tap
			Set Node15_Interface_InterfaceID_1.nat_id = 0
			Set Node15_Interface_InterfaceID_1.if_type = 8023
