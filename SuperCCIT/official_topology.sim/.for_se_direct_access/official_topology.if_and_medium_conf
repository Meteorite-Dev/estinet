			Set Node9_MAC8023_InterfaceID_5.OutThrputLogFile = our_topology_test.mac8023_n9_i5_out_thrput.log
			Set Node9_MAC8023_InterfaceID_5.InOutThrputLogFile = our_topology_test.mac8023_n9_i5_inout_thrput.log
