			Set Node8_MAC8023_InterfaceID_6.OutThrputLogFile = our_topology_test.mac8023_n8_i6_out_thrput.log
			Set Node8_MAC8023_InterfaceID_6.InOutThrputLogFile = our_topology_test.mac8023_n8_i6_inout_thrput.log
			Set Node8_MAC8023_InterfaceID_6.LinktoHub = 0

		Module PHY : Node8_PHY_InterfaceID_6
			Set Node8_PHY_InterfaceID_6.datarate = 10_Mbps
			Set Node8_PHY_InterfaceID_6.BER = 0.0
			Set Node8_PHY_InterfaceID_6.PropDelay = 1.0
			Set Node8_PHY_InterfaceID_6.linkfail = off
			Set Node8_PHY_InterfaceID_6.linkfailFileName = our_topology_test.phy8023_n8_i6.link_failure_duration

		Module Link : Node8_Link_InterfaceID_6
			Set Node8_Link_InterfaceID_6.guitag_remotenode = yes


		Bind Node8_Interface_InterfaceID_6 Node8_MAC8023_InterfaceID_6
		Bind Node8_MAC8023_InterfaceID_6 Node8_PHY_InterfaceID_6
		Bind Node8_PHY_InterfaceID_6 Node8_Link_InterfaceID_6
	EndDefine

EndCreate


