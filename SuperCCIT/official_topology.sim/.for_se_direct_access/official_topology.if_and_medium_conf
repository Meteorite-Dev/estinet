			Set Node7_MAC8023_InterfaceID_6.OutThrputLogFile = our_topology_test.mac8023_n7_i6_out_thrput.log
			Set Node7_MAC8023_InterfaceID_6.InOutThrputLogFile = our_topology_test.mac8023_n7_i6_inout_thrput.log
			Set Node7_MAC8023_InterfaceID_6.LinktoHub = 0

