			Set Node16_MAC8023_InterfaceID_4.UniOutLogFile = our_topology_test.mac8023_n16_i4_uni_out.log
			Set Node16_MAC8023_InterfaceID_4.UniInOutLogFile = our_topology_test.mac8023_n16_i4_uni_inout.log
			Set Node16_MAC8023_InterfaceID_4.BroInLogFile = our_topology_test.mac8023_n16_i4_bro_in.log
