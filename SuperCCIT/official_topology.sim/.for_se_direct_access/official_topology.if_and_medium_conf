			Set Node5_MAC8023_InterfaceID_4.BroOutLogFile = our_topology_test.mac8023_n5_i4_bro_out.log
			Set Node5_MAC8023_InterfaceID_4.BroInOutLogFile = our_topology_test.mac8023_n5_i4_bro_inout.log
			Set Node5_MAC8023_InterfaceID_4.CollLogFile = our_topology_test.mac8023_n5_i4_collision.log
