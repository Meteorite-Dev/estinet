			Set Node24_MAC8023_InterfaceID_1.UniInOutLogFile = our_topology.mac8023_n24_i1_uni_inout.log
			Set Node24_MAC8023_InterfaceID_1.BroInLogFile = our_topology.mac8023_n24_i1_bro_in.log
			Set Node24_MAC8023_InterfaceID_1.BroOutLogFile = our_topology.mac8023_n24_i1_bro_out.log
			Set Node24_MAC8023_InterfaceID_1.BroInOutLogFile = our_topology.mac8023_n24_i1_bro_inout.log
