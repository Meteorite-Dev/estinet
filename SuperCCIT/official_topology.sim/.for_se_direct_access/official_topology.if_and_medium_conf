		Module MAC8023 : Node8_MAC8023_InterfaceID_6
			Set Node8_MAC8023_InterfaceID_6.mac = 00:01:00:00:00:61
			Set Node8_MAC8023_InterfaceID_6.lock_mac = off
			Set Node8_MAC8023_InterfaceID_6.PromisOpt = off
			Set Node8_MAC8023_InterfaceID_6.mode = full
			Set Node8_MAC8023_InterfaceID_6.log = off
			Set Node8_MAC8023_InterfaceID_6.logInterval = 1
			Set Node8_MAC8023_InterfaceID_6.NumCollision = off
			Set Node8_MAC8023_InterfaceID_6.NumUniInPkt = off
			Set Node8_MAC8023_InterfaceID_6.NumUniOutPkt = off
			Set Node8_MAC8023_InterfaceID_6.NumUniInOutPkt = off
			Set Node8_MAC8023_InterfaceID_6.NumBroInPkt = off
			Set Node8_MAC8023_InterfaceID_6.NumBroOutPkt = off
			Set Node8_MAC8023_InterfaceID_6.NumBroInOutPkt = off
			Set Node8_MAC8023_InterfaceID_6.NumDrop = off
