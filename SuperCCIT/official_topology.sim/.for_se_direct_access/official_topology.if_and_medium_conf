	Define InterfaceID 1
		Module Interface : Node9_Interface_InterfaceID_1
			Set Node9_Interface_InterfaceID_1.tunnel_type = tap
			Set Node9_Interface_InterfaceID_1.nat_id = 0
			Set Node9_Interface_InterfaceID_1.if_type = 8023
