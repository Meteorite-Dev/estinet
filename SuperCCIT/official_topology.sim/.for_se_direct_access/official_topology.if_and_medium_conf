Set RandomNumberSeed = 1
Set WireLogFlag = on
Set WirelessLogFlag = on
Set RunTimeFrameAnimationAndNodeMovement = off
Set WiFiEnableBitError11a = off
Set WiFiChannelCoding11a = on
Set WiFiEnableBitError11n = off
Set WiFiChannelCoding11n = on
Set WAVEEnableBitError = off
Set WAVEChannelCoding = off
Set RunTimeSDNGroupingStatus = off

Create Node 1 as OF_CTRL with name = OF_CTRL1
	Define InterfaceID 1
		Module Interface : Node1_Interface_InterfaceID_1
			Set Node1_Interface_InterfaceID_1.tunnel_type = tap
			Set Node1_Interface_InterfaceID_1.nat_id = 0
			Set Node1_Interface_InterfaceID_1.if_type = 8023
			Set Node1_Interface_InterfaceID_1.if_name = eth0
			Set Node1_Interface_InterfaceID_1.max_qlen = 50
			Set Node1_Interface_InterfaceID_1.log_qlen = off
			Set Node1_Interface_InterfaceID_1.log_option = FullLog
			Set Node1_Interface_InterfaceID_1.samplerate = 1
			Set Node1_Interface_InterfaceID_1.logFileName = our_topology.interface_n1_i1_qlen.log
			Set Node1_Interface_InterfaceID_1.EnableClassify = on
			Set Node1_Interface_InterfaceID_1.ClassifyFileName = our_topology.n1_i1.flow_rule

		Module MAC8023 : Node1_MAC8023_InterfaceID_1
			Set Node1_MAC8023_InterfaceID_1.mac = 00:01:00:00:00:01
			Set Node1_MAC8023_InterfaceID_1.lock_mac = off
			Set Node1_MAC8023_InterfaceID_1.PromisOpt = off
			Set Node1_MAC8023_InterfaceID_1.mode = full
			Set Node1_MAC8023_InterfaceID_1.log = off
			Set Node1_MAC8023_InterfaceID_1.logInterval = 1
			Set Node1_MAC8023_InterfaceID_1.NumCollision = off
			Set Node1_MAC8023_InterfaceID_1.NumUniInPkt = off
			Set Node1_MAC8023_InterfaceID_1.NumUniOutPkt = off
			Set Node1_MAC8023_InterfaceID_1.NumUniInOutPkt = off
			Set Node1_MAC8023_InterfaceID_1.NumBroInPkt = off
			Set Node1_MAC8023_InterfaceID_1.NumBroOutPkt = off
			Set Node1_MAC8023_InterfaceID_1.NumBroInOutPkt = off
			Set Node1_MAC8023_InterfaceID_1.NumDrop = off
			Set Node1_MAC8023_InterfaceID_1.InThrput = off
			Set Node1_MAC8023_InterfaceID_1.OutThrput = off
			Set Node1_MAC8023_InterfaceID_1.InOutThrput = off
			Set Node1_MAC8023_InterfaceID_1.UniInLogFile = our_topology.mac8023_n1_i1_uni_in.log
			Set Node1_MAC8023_InterfaceID_1.UniOutLogFile = our_topology.mac8023_n1_i1_uni_out.log
			Set Node1_MAC8023_InterfaceID_1.UniInOutLogFile = our_topology.mac8023_n1_i1_uni_inout.log
			Set Node1_MAC8023_InterfaceID_1.BroInLogFile = our_topology.mac8023_n1_i1_bro_in.log
			Set Node1_MAC8023_InterfaceID_1.BroOutLogFile = our_topology.mac8023_n1_i1_bro_out.log
			Set Node1_MAC8023_InterfaceID_1.BroInOutLogFile = our_topology.mac8023_n1_i1_bro_inout.log
			Set Node1_MAC8023_InterfaceID_1.CollLogFile = our_topology.mac8023_n1_i1_collision.log
			Set Node1_MAC8023_InterfaceID_1.DropLogFile = our_topology.mac8023_n1_i1_drop.log
			Set Node1_MAC8023_InterfaceID_1.InThrputLogFile = our_topology.mac8023_n1_i1_in_thrput.log
			Set Node1_MAC8023_InterfaceID_1.OutThrputLogFile = our_topology.mac8023_n1_i1_out_thrput.log
			Set Node1_MAC8023_InterfaceID_1.InOutThrputLogFile = our_topology.mac8023_n1_i1_inout_thrput.log
			Set Node1_MAC8023_InterfaceID_1.LinktoHub = 0

		Module PHY : Node1_PHY_InterfaceID_1
			Set Node1_PHY_InterfaceID_1.datarate = 10_Mbps
			Set Node1_PHY_InterfaceID_1.BER = 0.0
			Set Node1_PHY_InterfaceID_1.PropDelay = 1.0
			Set Node1_PHY_InterfaceID_1.linkfail = off
			Set Node1_PHY_InterfaceID_1.linkfailFileName = our_topology.phy8023_n1_i1.link_failure_duration

		Module Link : Node1_Link_InterfaceID_1
			Set Node1_Link_InterfaceID_1.guitag_remotenode = yes


		Bind Node1_Interface_InterfaceID_1 Node1_MAC8023_InterfaceID_1
		Bind Node1_MAC8023_InterfaceID_1 Node1_PHY_InterfaceID_1
		Bind Node1_PHY_InterfaceID_1 Node1_Link_InterfaceID_1
	EndDefine

EndCreate


Create Node 2 as OF_CTRL_SW with name = OF_CTRL_SW2
	Define InterfaceID 1
		Module Interface : Node2_Interface_InterfaceID_1
			Set Node2_Interface_InterfaceID_1.tunnel_type = tap
			Set Node2_Interface_InterfaceID_1.nat_id = 0
			Set Node2_Interface_InterfaceID_1.if_type = 8023
			Set Node2_Interface_InterfaceID_1.if_name = eth0
			Set Node2_Interface_InterfaceID_1.max_qlen = 50
			Set Node2_Interface_InterfaceID_1.log_qlen = off
			Set Node2_Interface_InterfaceID_1.log_option = FullLog
			Set Node2_Interface_InterfaceID_1.samplerate = 1
			Set Node2_Interface_InterfaceID_1.logFileName = our_topology.interface_n2_i1_qlen.log
			Set Node2_Interface_InterfaceID_1.EnableClassify = on
			Set Node2_Interface_InterfaceID_1.ClassifyFileName = our_topology.n2_i1.flow_rule

		Module MAC8023 : Node2_MAC8023_InterfaceID_1
			Set Node2_MAC8023_InterfaceID_1.mac = 00:01:00:00:00:02
			Set Node2_MAC8023_InterfaceID_1.lock_mac = off
			Set Node2_MAC8023_InterfaceID_1.PromisOpt = off
			Set Node2_MAC8023_InterfaceID_1.mode = full
			Set Node2_MAC8023_InterfaceID_1.log = off
			Set Node2_MAC8023_InterfaceID_1.logInterval = 1
			Set Node2_MAC8023_InterfaceID_1.NumCollision = off
			Set Node2_MAC8023_InterfaceID_1.NumUniInPkt = off
			Set Node2_MAC8023_InterfaceID_1.NumUniOutPkt = off
			Set Node2_MAC8023_InterfaceID_1.NumUniInOutPkt = off
			Set Node2_MAC8023_InterfaceID_1.NumBroInPkt = off
			Set Node2_MAC8023_InterfaceID_1.NumBroOutPkt = off
			Set Node2_MAC8023_InterfaceID_1.NumBroInOutPkt = off
			Set Node2_MAC8023_InterfaceID_1.NumDrop = off
			Set Node2_MAC8023_InterfaceID_1.InThrput = off
			Set Node2_MAC8023_InterfaceID_1.OutThrput = off
			Set Node2_MAC8023_InterfaceID_1.InOutThrput = off
			Set Node2_MAC8023_InterfaceID_1.UniInLogFile = our_topology.mac8023_n2_i1_uni_in.log
			Set Node2_MAC8023_InterfaceID_1.UniOutLogFile = our_topology.mac8023_n2_i1_uni_out.log
			Set Node2_MAC8023_InterfaceID_1.UniInOutLogFile = our_topology.mac8023_n2_i1_uni_inout.log
			Set Node2_MAC8023_InterfaceID_1.BroInLogFile = our_topology.mac8023_n2_i1_bro_in.log
			Set Node2_MAC8023_InterfaceID_1.BroOutLogFile = our_topology.mac8023_n2_i1_bro_out.log
			Set Node2_MAC8023_InterfaceID_1.BroInOutLogFile = our_topology.mac8023_n2_i1_bro_inout.log
			Set Node2_MAC8023_InterfaceID_1.CollLogFile = our_topology.mac8023_n2_i1_collision.log
			Set Node2_MAC8023_InterfaceID_1.DropLogFile = our_topology.mac8023_n2_i1_drop.log
			Set Node2_MAC8023_InterfaceID_1.InThrputLogFile = our_topology.mac8023_n2_i1_in_thrput.log
			Set Node2_MAC8023_InterfaceID_1.OutThrputLogFile = our_topology.mac8023_n2_i1_out_thrput.log
			Set Node2_MAC8023_InterfaceID_1.InOutThrputLogFile = our_topology.mac8023_n2_i1_inout_thrput.log
			Set Node2_MAC8023_InterfaceID_1.LinktoHub = 0

		Module PHY : Node2_PHY_InterfaceID_1
			Set Node2_PHY_InterfaceID_1.datarate = 10_Mbps
			Set Node2_PHY_InterfaceID_1.BER = 0.0
			Set Node2_PHY_InterfaceID_1.PropDelay = 1.0
			Set Node2_PHY_InterfaceID_1.linkfail = off
			Set Node2_PHY_InterfaceID_1.linkfailFileName = our_topology.phy8023_n2_i1.link_failure_duration

		Module Link : Node2_Link_InterfaceID_1
			Set Node2_Link_InterfaceID_1.guitag_remotenode = yes


		Bind Node2_Interface_InterfaceID_1 Node2_MAC8023_InterfaceID_1
		Bind Node2_MAC8023_InterfaceID_1 Node2_PHY_InterfaceID_1
		Bind Node2_PHY_InterfaceID_1 Node2_Link_InterfaceID_1
	EndDefine

	Define InterfaceID 2
