			Set Node2_MAC8023_InterfaceID_16.InOutThrput = off
			Set Node2_MAC8023_InterfaceID_16.UniInLogFile = our_topology_test.mac8023_n2_i16_uni_in.log
			Set Node2_MAC8023_InterfaceID_16.UniOutLogFile = our_topology_test.mac8023_n2_i16_uni_out.log
			Set Node2_MAC8023_InterfaceID_16.UniInOutLogFile = our_topology_test.mac8023_n2_i16_uni_inout.log
			Set Node2_MAC8023_InterfaceID_16.BroInLogFile = our_topology_test.mac8023_n2_i16_bro_in.log
			Set Node2_MAC8023_InterfaceID_16.BroOutLogFile = our_topology_test.mac8023_n2_i16_bro_out.log
			Set Node2_MAC8023_InterfaceID_16.BroInOutLogFile = our_topology_test.mac8023_n2_i16_bro_inout.log
