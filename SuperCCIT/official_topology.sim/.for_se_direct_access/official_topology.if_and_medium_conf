			Set Node7_MAC8023_InterfaceID_6.BroInOutLogFile = our_topology_test.mac8023_n7_i6_bro_inout.log
			Set Node7_MAC8023_InterfaceID_6.CollLogFile = our_topology_test.mac8023_n7_i6_collision.log
			Set Node7_MAC8023_InterfaceID_6.DropLogFile = our_topology_test.mac8023_n7_i6_drop.log
