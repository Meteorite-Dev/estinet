			Set Node2_MAC8023_InterfaceID_16.OutThrputLogFile = our_topology_test.mac8023_n2_i16_out_thrput.log
			Set Node2_MAC8023_InterfaceID_16.InOutThrputLogFile = our_topology_test.mac8023_n2_i16_inout_thrput.log
