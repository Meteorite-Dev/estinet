Create Node 16 as Open_vSwitch with name = Open_vSwitch16
	Define InterfaceID 1
		Module Interface : Node16_Interface_InterfaceID_1
			Set Node16_Interface_InterfaceID_1.tunnel_type = tap
			Set Node16_Interface_InterfaceID_1.nat_id = 0
